class Solution {
    public void moveZeroes(int[] nums) {
        
        int prev = 0;
        
        for(int i=0;i< nums.length;i++){
            if(nums[i] != 0){
                nums[prev]  = nums[i];
                prev++;
            }
        }
        
        for(;prev<nums.length;prev++){
            nums[prev] = 0;
        }
        
    }
}
